{
    "name": "testPortalAntyspy",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"] 
        }],
        
    "web_accessible_resources": ["pageScript.js"],
    "browser_action":{
      "default_title": "Testportal Antyspy",
      "default_popup": "popup.html"
    },
    "permissions": [
      "storage"
    ],
    "icons": { 
     "128": "assets/icon.png" }
  }